{
    "name": "OG Tag Preview",
    "description": "If you use this tool, you can preview the OG tag on your website",
    "version": "1.1",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "<all_urls>",
        "tabs"
    ]
}